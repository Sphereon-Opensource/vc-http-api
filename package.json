{
  "name": "vc-http-api",
  "version": "1.1.0",
  "description": "Issue VCs and verify all W3C compliant credentials",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "Sphereon/vc-http-api",
  "author": "Scott Malley <smalley@sphereon.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "bs58": "^4.0.1",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "handlebars": "^4.7.6",
    "husky": "^4.2.4",
    "jsonld": "^3.1.0",
    "jsonld-signatures": "^5.0.1",
    "morgan": "^1.8.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.6",
    "promise": "^8.1.0",
    "resource-router-middleware": "^0.6.0",
    "tweetnacl": "^1.0.3",
    "uuid": "^7.0.3",
    "vc-js": "^0.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.19.4"
  }
}
